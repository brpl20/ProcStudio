<div id="example-basic">
  <h3>Dados do Contador</h3>
  <section>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :lastname %>
    <%= f.text_field :lastname %>
  </section>

  <%= render partial: 'shared/contacts', locals: { f: f }  %>

  <h3>Extras</h3>
  <section>
    <%= check_box_tag :flag_access_data, 1, true, id: 'access-email' %>
    <label for="access-email"> Enviar os dados de acesso </label>

    <%= check_box_tag :flag_generate_documents, 1, false, id: 'access-docs' %>
    <label for="access-docs"> Emitir documentos </label>

    <%= check_box_tag :flag_signature, 1, false, id: 'access-data' %>
    <label for="access-data"> Enviar para assinatura </label>

  </section>

</div>
